    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "_moduleAliases": {
    "@config": "./backend/config",
    "@routes": "./backend/routes",
    "@models": "./backend/models",
    "@utils": "./backend/utils"
  },
  "lint-staged": {
    "backend/**/*.ts": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ]
  }
}
